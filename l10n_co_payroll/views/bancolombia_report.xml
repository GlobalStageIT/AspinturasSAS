<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.actions.act_window" id="action_bancolombia_report">
            <field name="name">Informe de Bancolombia</field>
            <field name="res_model">bancolombia.report</field>
            <field name="view_mode">tree</field>
        </record>

        <record id="bancolombia_report_tree_view" model="ir.ui.view">
            <field name="name">bancolombia.report.tree</field>
            <field name="model">bancolombia.report</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="dato"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>